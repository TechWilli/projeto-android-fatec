<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fatecanos.alunosmobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.fatecanos.alunosmobile.telas.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.fatecanos.alunosmobile.telas.AddUsuActivity"
            android:label="@string/title_activity_add_usu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.fatecanos.alunosmobile.telas.MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.fatecanos.alunosmobile.telas.ListUsuActivity"
            android:label="@string/title_activity_list_usu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.fatecanos.alunosmobile.telas.UptUsuActivity"
            android:label="@string/title_activity_upt_usu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.fatecanos.alunosmobile.telas.ListUsuParamActivity"
            android:label="@string/title_activity_list_usu_param"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>